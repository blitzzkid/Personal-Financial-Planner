{
  "env": {
    "node": true,
    "commonjs": true,
    "es6": true,
    "jest/globals": true,
    "cypress/globals": true
  },
  "extends": ["react-app", "plugin:cypress/recommended", "eslint:recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["jest", "cypress"],
  "rules": {}
}
